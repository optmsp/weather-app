    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.3",

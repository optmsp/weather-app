{
  "name": "weather-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{vue,js,ts,json,css}\" \"**/*.md\"",
    "format:check": "prettier --check \"src/**/*.{vue,js,ts,json,css}\" \"**/*.md\"",
    "mock-api": "json-server --watch db.json --port 3000",
    "ci": "npm run type-check && npm run build",
    "prepare": "npm run format"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@vue/cli-service": "~5.0.8",
